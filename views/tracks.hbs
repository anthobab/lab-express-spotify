<h1>Album : {{album}}</h1>
<div class="flex">
  {{#each tracks}}
    <div class="album-div tracks">
      <p>{{this.name}}</p>
      <figure class="tracks">
        <figcaption>Listen 30s extraxct:</figcaption>
        <audio controls src="{{this.preview_url}}">
          <a href="">
            Download audio
          </a>
        </audio>
      </figure>
    </div>
  {{/each}}
</div>